<?xml version="1.0" encoding="UTF-8"?>
<webApp>
    <webContainer>
        <name>web</name>
        <connectWith>data</connectWith>
        <url>localhost:4848</url>
    </webContainer>

    <dataContainer>
        <name>data</name>
        <connectWith>web</connectWith>
        <dbPort>3306</dbPort>
        <dbUrl>jdbc:mysql://localhost:3306/test?profileSQL=true</dbUrl>
        <dbUsername>root</dbUsername>
        <dbPassword>pass</dbPassword>
        <dbName>test</dbName>
    </dataContainer>

    <containersNum>2</containerNum>
</webApp>